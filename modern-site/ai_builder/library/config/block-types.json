{
  "blockTypes": {
    "header": {
      "category": "navigation",
      "required": true,
      "position": "top",
      "variants": ["generic/header.html"],
      "description": "Site navigation and header",
      "parameters": {
        "required": ["businessName"],
        "optional": ["logoUrl", "navLinks", "ctaText", "ctaLink"]
      }
    },
    "hero": {
      "category": "hero",
      "required": true,
      "position": "top",
      "variants": [
        "generic/hero.html",
        "signature/hero-cinematic.html",
        "signature/hero-glass.html",
        "signature/parallax-hero.html",
        "signature/video-hero.html"
      ],
      "description": "Main hero section with call-to-action",
      "parameters": {
        "required": ["heroTitle", "heroSubtitle"],
        "optional": ["heroImage", "heroAlt", "ctaPrimaryText", "ctaPrimaryLink", "ctaSecondaryText", "ctaSecondaryLink", "backgroundColor", "primaryColor"]
      },
      "variantDetails": {
        "generic/hero.html": {
          "style": "standard",
          "useCase": "General purpose, works for all industries"
        },
        "signature/hero-cinematic.html": {
          "style": "premium",
          "useCase": "High-end, luxury brands, restaurants, hotels",
          "requires": ["heroImage"]
        },
        "signature/hero-glass.html": {
          "style": "modern",
          "useCase": "Tech startups, creative agencies, modern brands"
        },
        "signature/parallax-hero.html": {
          "style": "interactive",
          "useCase": "Portfolio sites, creative agencies, high-impact pages",
          "requires": ["heroImage", "JavaScript for parallax"]
        }
      }
    },
    "features": {
      "category": "content",
      "required": false,
      "position": "middle",
      "variants": [
        "generic/features.html",
